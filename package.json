{
  "name": "cheminfo-migration",
  "version": "0.0.0",
  "description": "Migrate simple JS projects to Typescript",
  "main": "./bin/bin.cjs",
  "bin": "./bin/bin.cjs",
  "keywords": [
    "cheminfo-migration",
    "cheminfo",
    "migration"
  ],
  "author": "Santi Miranda",
  "license": "MIT",
  "files": [
    "src",
    "bin"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib-esm",
    "eslint": "eslint src --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc && rollup -c rollup.config.js",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "tidy": "npm run eslint-fix && npm run prettier-write && npm run check-types",
    "test-only": "jest --coverage",
    "tsc-esm": "tsc --project tsconfig.esm.json",
    "tsc": "npm run clean && npm run tsc-esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/cheminfo-migration.git"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/cheminfo-migration/issues"
  },
  "homepage": "https://github.com/cheminfo/cheminfo-migration#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.2",
    "eslint": "^8.43.0",
    "eslint-config-cheminfo-typescript": "^11.3.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup": "^3.25.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "inquirer": "^9.2.7",
    "yaml": "^2.3.1"
  },
  "type": "module"
}
